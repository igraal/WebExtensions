{
    "manifest_version": 2,
    "name": "WebExtensions sample",
    "description": "WebExtensions sample",
    "version": "1.0",
    "minimum_chrome_version": "23",
    "browser_action": {
        "default_title": "WebExtensions sample",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}